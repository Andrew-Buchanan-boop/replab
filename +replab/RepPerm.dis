classdef PermRep < URep
    
    properties (SetAccess = private) 
        % private set access because Octave doesn't support immutable
        d;
        field;
        group;
    end
    
    methods
        
        function self = PermRep(generators, field)
            self.d = size(generators, 2);
            % No support for the complex field yet
            assert(isequal(field, 'R'));
            self.field = field;
        end
        
    end
        
end
